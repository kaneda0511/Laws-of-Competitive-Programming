   "source": [
    "N,K = map(int,input().split())\n",
    "ans = 0\n",
    "for x in range(1,N+1):\n",
    "    for y in range(1,N+1):\n",
    "        z = K - x - y\n",
    "        if 0<z and z<=N:\n",
    "            ans += 1\n",
    "print(ans)"
