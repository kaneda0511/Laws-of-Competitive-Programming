    "N = int(input())\n",
    "h = list(map(int,input().split()))\n",
    "dp = [0]*N\n",
    "dp[0] = 0\n",
    "dp[1] = abs(h[0]-h[1])\n",
    "for i in range(2,N):\n",
    "    dp[i] = min(dp[i-1]+abs(h[i-1]-h[i]),dp[i-2]+abs(h[i-2]-h[i]))\n",
    "print(dp[N-1])"
   ]
