    "def f(x):\n",
    "    return x**3+x\n",
    "\n",
    "N = int(input())\n",
    "left=0\n",
    "right=100\n",
    "for i in range(10000):\n",
    "    mid = (left+right)/2\n",
    "    if N<f(mid):\n",
    "        right=mid\n",
    "    if f(mid)<N:\n",
    "        left=mid\n",
    "\n",
    "print(left)"
   ]
