   "source": [
    "N = int(input())\n",
    "L=[]\n",
    "for i in range(1000):\n",
    "    \n",
    "    a = N//(2**i)\n",
    "    m = a%2\n",
    "    L.append(str(m))\n",
    "    if a == 0 or a == 1:\n",
    "        break\n",
    "\n",
    "n=10-len(L)\n",
    "if 0<n:\n",
    "    for i in range(n):\n",
    "        L.append(str(0))\n",
    "\n",
    "ans = L[::-1]\n",
    "print(\"\".join(ans))\n",
    "\n",
    "\n"
   ]
