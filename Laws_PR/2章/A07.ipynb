   "source": [
    "D = int(input())\n",
    "N = int(input())\n",
    "G = [0]*(D+2)\n",
    "for i in range(N):\n",
    "    L,R = map(int,input().split())\n",
    "    G[L]+=1\n",
    "    G[R+1]-=1\n",
    "\n",
    "ans = [0]*(D+2)\n",
    "for i in range(1,D+1):\n",
    "    ans[i] = ans[i-1] + G[i]\n",
    "\n",
    "for i in range(1,D+1):\n",
    "    print(ans[i])"
   ]
